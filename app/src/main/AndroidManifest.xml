<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.audicoastoverdrivedirector">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/audi_coast"
        android:label="@string/app_name"
        android:roundIcon="@drawable/audi_coast"
        android:supportsRtl="true"
        android:theme="@style/Theme.AudicoastOverdriveDirector">
        <activity android:name=".about"></activity>
        <activity android:name=".MusicPlayActivity">
            android:requestLegacyExternalStorage="true"
        </activity>
        <activity android:name=".Video" />
        <activity android:name=".Music" />
        <activity android:name=".Image" />
        <activity android:name=".Folder" />
        <activity android:name=".CreateFolder" />
        <activity android:name=".MusicActivity" />
        <activity android:name=".FileActivity" />
        <activity android:name=".forget_password" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".sign_up" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>